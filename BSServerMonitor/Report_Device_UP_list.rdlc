      <Query>
        <DataSourceName>bssmConnectionString</DataSourceName>
        <CommandText>select `list_servers`.`ID` AS `ID`,`list_servers`.`ServerName` AS `ServerName`, `list_servers`.`ServerIPAddress` AS `ServerIPAddress`,(case `list_servers`.`CS` when 0 then 'Down' when 1 then 'Up' when 2 then 'Changed/Warning' when 3 then 'Disabled' when 5 then 'Deleted' end) AS `CurrentStatus`,`list_servers`.`DisplayName` AS `DisplayName`,(case `list_servers`.`IsEnabled` when 0 then 'Disabled' when 1 then 'Enabled' end) AS `Enabled`,`list_servers`.`DTAdded` AS `DTAdded`,(case `list_servers`.`DoPing` when 0 then 'No' when 1 then 'Yes' end) AS `Do Ping`,(case `list_servers`.`DoTrace` when 0 then 'No' when 1 then 'Yes' end) AS `Do Trace`,(case `list_servers`.`DoPort` when 0 then 'No' when 1 then 'Yes' end) AS `Do Port`,(case `list_servers`.`DoHTTP` when 0 then 'No' when 1 then 'Yes' end) AS `Do HTTP`,`list_servers_types`.`Cat` AS `Category` from (`list_servers` join `list_servers_types` on((`list_servers`.`TID` = `list_servers_types`.`ID`)))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
